    "masterdata": {
        "object_mode": 0,
        "add_empty_rec": 0,
        "sql_statement":"select k8references.*, ID as image_ID, concat(path,'/',filename) as image_file, filename as image_filename, case when width>height then 'landscape' else 'portrait' end as image_orientation, width/height as image_aspectratio, width as image_width, height as image_height, 1 as image_count from k8references WHERE 1=1 order by basetype,baseid,sort",
